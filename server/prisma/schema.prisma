generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id   String @id @default(uuid())
  name String @unique

  @@map("users")
}

model Bets {
  id          String @id @default(uuid())
  title       String
  value       String
  description String

  @@map("bets")
}
